<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.naufal.kidsnesia.main_features.presentation.dashboard.DashboardFragment"
        android:label="@string/nav_dashboard"
        tools:layout="@layout/fragment_dashboard"/>

    <fragment
        android:id="@+id/navigation_video"
        android:name="com.naufal.kidsnesia.main_features.presentation.video.VideoFragment"
        android:label="@string/nav_event"
        tools:layout="@layout/fragment_video" />

    <fragment
        android:id="@+id/navigation_event"
        android:name="com.naufal.kidsnesia.main_features.presentation.event.EventFragment"
        android:label="@string/nav_event"
        tools:layout="@layout/fragment_event" />

    <fragment
        android:id="@+id/navigation_profile"
        android:name="com.naufal.kidsnesia.auth.presentasi.profile.ProfileFragment"
        android:label="@string/nav_event"
        tools:layout="@layout/fragment_profile" />

</navigation>